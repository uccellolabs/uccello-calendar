{
    "name": "uccello/calendar",
    "description": "A synchronized calendar for Uccello.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "uccello/uccello": "1.0.*",
        "league/oauth2-client": "^2.4",
        "google/apiclient": "^2.2",
        "microsoft/microsoft-graph": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\Calendar\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uccello\\Calendar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\Calendar\\Providers\\AppServiceProvider",
                "Uccello\\Calendar\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
